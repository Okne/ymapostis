import * from "space_base.material"

material space/SkyBox
{
	technique
	{
		pass
		{
			lighting off
			depth_write off
			texture_unit
			{
				cubic_texture skybox_FR.jpg skybox_BK.jpg skybox_LF.jpg skybox_RT.jpg skybox_UP.jpg skybox_DN.jpg separateUV
				tex_address_mode clamp
			}
		}
	}
}

material space/None
{
	technique
	{
		pass
		{
			lighting on
			ambient 0.0 0.0 0.0 0.0
			diffuse 1.0 0.0 0.0 0.0
			depth_write on
			
		}
	}
}

material space/Sun_Normal: space_Normal
{
	technique
	{
		pass
		{
			ambient 0.000 0.000 0.000 0.000
			depth_write on
			texture_unit
			{
				texture sunmap.jpg
				filtering bilinear
			}
		}
	}
}

